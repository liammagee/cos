@prefix sdb:     <http://jena.hpl.hp.com/2007/sdb#> .@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix ja:      <http://jena.hpl.hp.com/2005/11/Assembler#> .# SDB for PostgreSQL over the simple layout2.<#store> rdf:type sdb:Store ;    sdb:layout         "layout2/hash" ;    sdb:connection     <#conn> ;.<#conn> rdf:type sdb:SDBConnection ;    sdb:sdbType        "postgresql" ;    sdb:sdbHost        "localhost" ;    sdb:sdbPort        "5432" ;    sdb:sdbName        "mydb" ;    sdb:driver         "org.postgresql.Driver" ;    sdb:sdbUser        "postgres" ;    sdb:sdbPassword    "1234" .